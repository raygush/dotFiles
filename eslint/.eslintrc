{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            0,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
		"strict": 0,
		"no-underscore-dangle": 0
    },
    "env": {
        "node": true,
        "browser": true
    }
}
